<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">

        <template id="voucher_report">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="l10n_bo_acoim_voucher.l10n_bo_voucher_format" />
                </t>
            </t>
        </template>

        <record id="ir_actions_report_voucher" model="ir.actions.report">
            <field name="name">Comprobante (BO)</field>
            <field name="model">account.move</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">l10n_bo_acoim_voucher.voucher_report</field>
            <field name="report_file">l10n_bo_acoim_voucher.voucher_report</field>
            <field name="paperformat_id" ref="l10n_bo_acoim_voucher.voucher_paperformat_id"/>
            <field name="binding_type">report</field>
            <field name="binding_model_id" ref="model_account_move"/>
        </record>
    </data>
</odoo>
