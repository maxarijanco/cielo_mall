<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">

        <record id="view_l10n_bo_cufd" model="ir.actions.act_window">
            <field name="name">CUFDs</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">l10n.bo.cufd</field>
            <field name="view_mode">tree</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Crea tu primer CUFD
                </p>
            </field>
        </record>
        <record id="view_l10n_bo_cufd_tree" model="ir.ui.view">
            <field name="name">l10n.bo.cufd.tree</field>
            <field name="model">l10n.bo.cufd</field>
            <field name="arch" type="xml">
                <tree create="false" >
                    <field name="name" readonly="1" />
                    <field name="fechaSolicitud"/>
                    <field name="fechaVigencia" />
                </tree>
            </field>
        </record>


    </data>
</odoo>
