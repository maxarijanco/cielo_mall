<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">

            
        <record id="view_l10n_bo_supplier_message_line" model="ir.actions.act_window">
            <field name="name">Lineas de mensaje de paquete de facturas de proveedor</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">l10n.bo.supplier.package.message</field>
            <field name="view_mode">tree</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Crea tu primer linea de mensaje de paquete de factura de proveedor
                </p>
            </field>
        </record>

        <record id="view_l10n_bo_supplier_message_line_tree" model="ir.ui.view">
            <field name="model">l10n.bo.supplier.package.message</field>
            <field name="arch" type="xml">
                <tree editable="bottom" create="false" >
                    <field name="name" />
                    <field name="code" />
                    <field name="description" />
                </tree>
            </field>
        </record>

    </data>
</odoo>
