<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">
        <record id="view_l10n_bo_purchase_register_line" model="ir.actions.act_window">
            <field name="name">Lineas de registro de compra (BO)</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">l10n.bo.purchase.register.line</field>
            <field name="view_mode">tree</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Crea tu primer linea registro de compra (BO).
                </p>
            </field>
        </record>
        <record id="view_l10n_bo_purchase_register_line_tree" model="ir.ui.view">
            <field name="name">view_l10n_bo_purchase_register_line_tree</field>
            <field name="model">l10n.bo.purchase.register.line</field>
            <field name="arch" type="xml">
                <tree create="false" >
                    <field name="invoice_id" column_invisible="True"/>
                    <field name="company_id" column_invisible="True" />
                    <field name="type" column_invisible="True"/>
                    <field name="name"/>
                    <field name="specification"/>
                    <field name="nit"/>
                    <field name="reazon_social"/>
                    <field name="autorization_code"/>
                    <field name="invoice_number"/>
                    <field name="dui_dim_number"/>
                    <field name="dui_dim_date"/>
                    <field name="amount_total"/>
                    <field name="amount_ice"/>
                    <field name="amount_iehd"/>
                    <field name="amount_ipj"/>
                    <field name="amount_rate"/>
                    <field name="amount_no_iva"/>
                    <field name="amount_exempt"/>
                    <field name="amount_cero_rate"/>
                    <field name="amount_subtotal"/>
                    <field name="amount_discount"/>
                    <field name="amount_gift_card"/>
                    <field name="amount_base_fiscal_credit"/>
                    <field name="amount_fiscal_credit"/>
                    <field name="purchase_type"/>
                    <field name="control_code"/>
                    

                    
                </tree>
            </field>
        </record>

    </data>
</odoo>
